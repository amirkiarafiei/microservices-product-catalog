SERVICE_NAME=identity-service
LOG_LEVEL=DEBUG
DATABASE_URL=postgresql://user:password@localhost:5432/identity_db
JWT_ALGORITHM=RS256
# 1 hour in minutes
ACCESS_TOKEN_EXPIRE_MINUTES=60

# RSA Private Key (Keep this secret!)
# Format: "-----BEGIN RSA PRIVATE KEY-----\nBASE64_ENCODED_DATA\n-----END RSA PRIVATE KEY-----"
JWT_PRIVATE_KEY="YOUR_PRIVATE_KEY_HERE"

# RSA Public Key (Used by other services to verify)
# Format: "-----BEGIN PUBLIC KEY-----\nBASE64_ENCODED_DATA\n-----END PUBLIC KEY-----"
JWT_PUBLIC_KEY="YOUR_PUBLIC_KEY_HERE"
